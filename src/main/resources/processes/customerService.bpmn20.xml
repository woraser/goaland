      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${isEnginee}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="examine" name="领导审批" activiti:assignee="${depManager}"></userTask>
    <sequenceFlow id="flow8" name="否" sourceRef="exclusivegateway1" targetRef="examine">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!isEnginee}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow9" sourceRef="examine" targetRef="evaluating"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow10" sourceRef="repair" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow11" name="不转派" sourceRef="exclusivegateway2" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${!entrust}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="entrust" name="转派" activiti:assignee="${entruster}"></userTask>
    <sequenceFlow id="flow12" name="转派" sourceRef="exclusivegateway2" targetRef="entrust">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${entrust}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow13" sourceRef="entrust" targetRef="endevent1"></sequenceFlow>
  </process>
